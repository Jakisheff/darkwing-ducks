# DarkwingDucks Environment Configuration
# Copy this file to .env and fill in your values

# =============================================================================
# RPC CONFIGURATION
# =============================================================================

# Helius API Key (get from: https://dev.helius.xyz/)
# Provides enhanced RPC features, priority fee estimation, better reliability
HELIUS_API_KEY=your-helius-api-key-here

# Alternative: Custom RPC URL (if not using Helius)
# RPC_URL=https://api.mainnet-beta.solana.com

# =============================================================================
# JITO CONFIGURATION
# =============================================================================

# Jito Block Engine endpoint (default: Amsterdam)
# JITO_ENGINE_URL=https://amsterdam.mainnet.block-engine.jito.wtf

# Jito tip account (mainnet)
# JITO_TIP_ACCOUNT=96gYZGLnJYVFmbjzopPSU6QiEV5fGqZNyN9nmNhvrZU5

# =============================================================================
# COMPLIANCE (Range Protocol)
# =============================================================================

# Enable/disable Range API sanctions screening
RANGE_ENABLED=true

# Range API base URL
# RANGE_API_URL=https://api.range.org/v1

# =============================================================================
# SERVER CONFIGURATION
# =============================================================================

# Server binding
SERVER_HOST=127.0.0.1
SERVER_PORT=8080

# Rate limiting
RATE_LIMIT_REQUESTS=10
RATE_LIMIT_WINDOW_SECS=60

# =============================================================================
# SECURITY
# =============================================================================

# Relayer keypair path (DO NOT COMMIT THE ACTUAL FILE)
RELAYER_KEYPAIR_PATH=security/relayer-keypair.json
